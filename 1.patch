-+static bfd_reloc_status_type
-+avr32_final_link_relocate(reloc_howto_type *howto,
-+			  bfd *input_bfd,
-+			  asection *input_section,
-+			  bfd_byte *contents,
-+			  Elf_Internal_Rela *rel,
