-+	case R_AVR32_32:
-+	  if ((info->shared || h != NULL)
-+	      && (sec->flags & SEC_ALLOC))
-+	    {
-+	      if (htab->srelgot == NULL)
-+		{
-+		  htab->srelgot = create_dynamic_section(dynobj, ".rela.got",
-+							 bed->dynamic_sec_flags
-+							 | SEC_READONLY, 2);
-+		  if (htab->srelgot == NULL)
