-+	case BFD_RELOC_32:
-+	case BFD_RELOC_16:
-+	case BFD_RELOC_8:
-+	case BFD_RELOC_AVR32_DIFF32:
-+	case BFD_RELOC_AVR32_DIFF16:
-+	case BFD_RELOC_AVR32_DIFF8:
