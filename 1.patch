-+		{
-+		  h->readonly_reloc_sec = sec;
-+		  h->readonly_reloc_offset = rel->r_offset;
-+		}
-+
-+	      if (h != NULL)
-+		{
-+		  pr_debug("Non-GOT reference to symbol %s\n",
-+			   h->root.root.root.string);
