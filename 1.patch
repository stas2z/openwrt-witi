-+#define avr32_elf_insert_field(size, field, abfd, reloc_entry, data)	\
-+  do									\
-+    {									\
-+      unsigned long x;							\
-+      x = bfd_get_##size (abfd, data + reloc_entry->address);		\
-+      x &= ~reloc_entry->howto->dst_mask;				\
-+      x |= field & reloc_entry->howto->dst_mask;			\
