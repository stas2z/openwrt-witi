-+#ifdef DEBUG
-+	      fprintf(stderr, "Swapping symbols in fixup:\n");
-+	      print_fixup(fix);
-+#endif
-+	      tmp = fix->fx_addsy;
-+	      fix->fx_addsy = fix->fx_subsy;
-+	      fix->fx_subsy = tmp;
-+	      fix->fx_offset = -fix->fx_offset;
-+	    }
-+	}
-+    }
