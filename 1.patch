-+static unsigned long
-+parse_reglist(char *str, char **endptr, int (*parse_reg)(const char *))
-+{
-+  int reg_from, reg_to;
-+  unsigned long result = 0;
-+  char *p1, *p2, c;
-+
-+  while (*str)
-+    {
-+      for (p1 = str; *p1; p1++)
-+	if (*p1 == ',' || *p1 == '-')
-+	  break;
-+
-+      c = *p1, *p1 = 0;
-+      reg_from = parse_reg(str);
-+      *p1 = c;
-+
-+      if (reg_from < 0)
-+	break;
-+
-+      if (*p1 == '-')
-+	{
-+	  for (p2 = ++p1; *p2; p2++)
-+	    if (*p2 == ',')
-+	      break;
-+
-+	  c = *p2, *p2 = 0;
-+	  /* printf("going to parse reg_to from `%s'\n", p1); */
-+	  reg_to = parse_reg(p1);
-+	  *p2 = c;
-+
-+	  if (reg_to < 0)
-+	    break;
-+
-+	  while (reg_from <= reg_to)
-+	    result |= (1 << reg_from++);
-+	  p1 = p2;
-+	}
-+      else
-+	result |= (1 << reg_from);
-+
-+      str = p1;
-+      if (*str) ++str;
-+    }
-+
-+  if (endptr)
-+    *endptr = str;
-+
-+  return result;
-+}
